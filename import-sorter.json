{
  "generalConfiguration": {
    "sortOnBeforeSave": true,
    "exclude": [
      "typedefs"
    ]
  },
  "importStringConfiguration": {
    "hasSemicolon": false
  },
  "sortConfiguration": {
    "customOrderingRules": {
      "defaultOrderLevel": 10,
      "rules": [
        {
          "regex": "react|react-native",
          "orderLevel": 10
        },
        {
          "regex": "^(react|react-native)$",
          "orderLevel": 10
        },
        {
          "type": "importMember",
          "regex": "^$",
          "orderLevel": 20,
          "disableSort": true
        },
        {
          "regex": "redux",
          "orderLevel": 25
        },
        {
          "regex": "^(controllers/index)$",
          "orderLevel": 25
        },
        {
          "regex": "^(assets|common|config|controllers|images|utils|models)",
          "orderLevel": 30
        },
        {
          "regex": "^(styled-components)",
          "orderLevel": 35
        },
        {
          "regex": "@bernatfortet",
          "orderLevel": 35
        },
        {
          "regex": "styles",
          "orderLevel": 35
        },
        {
          "regex": "^(components)",
          "orderLevel": 40
        },
        {
          "regex": "^[.]",
          "orderLevel": 50
        }
      ]
    }
  }
}